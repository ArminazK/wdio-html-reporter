{
  "name": "wdio-html-format-reporter",
  "version": "0.1.0",
  "description": "A WebdriverIO plugin. Create a basic HTML formatted report",
  "main": "build/reporter.js",
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rm -rf ./build ./coverage",
    "compile": "babel lib/ -d build/",
    "release": "np patch",
    "release:patch": "np patch",
    "release:minor": "np minor",
    "release:major": "np major",
    "test": "mocha test/",
    "prepublish": "npm prune"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdriverio/wdio-html-format-reporter.git"
  },
  "directories": {
    "lib": "./lib"
  },
  "keywords": [
    "html",
    "reporter",
    "webdriverio",
    "wdio",
    "wdio-plugin",
    "wdio-reporter"
  ],
  "author": "Adrien Ruiz Gauder <caritsqa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdriverio/wdio-html-format-reporter/issues"
  },
  "homepage": "https://github.com/webdriverio/wdio-html-format-reporter#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "chai": "^4.1.2",
    "fs-extra": "^5.0.0",
    "handlebars": "^4.0.11",
    "lodash": "^4.17.5",
    "mocha": "^5.0.1",
    "moment": "^2.20.1",
    "moment-duration-format": "^2.2.1",
    "nightmare": "^2.10.0",
    "npm-run-all": "^4.1.2",
    "path": "^0.12.7",
    "sleep": "^5.1.1"
  },
  "devDependencies": {},
